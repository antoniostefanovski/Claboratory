// 1va
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int opagacki(int broj){
    
    if(broj<10) return 1;
    if(broj<100) return broj%10<broj/10%10;
    return broj%10<broj/10%10 == opagacki(broj/10) ? broj%10<broj/10%10 : 0;
    
}


int main()
{
    int nums;
    scanf("%d", &nums);

    int num;

    for(int i=0; i<nums; i++) {
        scanf("%d", &num);
        printf("%d\n", opagacki(num));
    }

    return 0;
}

// 2ra
#include <stdio.h>

int f(int broj,int k,int suma){
    if(broj<10){
        if(broj>k){
            printf("%d",broj);
            suma+=broj;
        }
        return suma;
    }
    suma = f(broj/10,k,suma);
    if(broj%10>k){
        printf("%d",broj%10);
        suma+=broj%10;
    }
    return suma;
}

int main(){
    int k,n;
    scanf("%d %d",&k,&n);
    for(int i=0;i<n;i++){
        int broj;
        scanf("%d",&broj);
        printf(" : %d\n",f(broj,k,0));
    }
    return 0;
}

// 3ta
#include <stdio.h>

int f(int broj){
    if(!broj) return 0;
    return broj%10+f(broj/100);
}

int main(){
    int n;
    scanf("%d",&n);
    printf("%d",f(n/10));
    return 0;
}

// 4ta
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int f(int broj){
    if(!broj) return 1;
    return broj%10*f(broj/100);
}

int main(){
    int n;
    scanf("%d",&n);
    printf("%d",f(n/10));
    return 0;
}

// 5ta

#include <stdio.h>


int rastecki(int broj){
    if(broj<10) return 1;
    if(broj<100) return broj%10>broj/10%10;
    return broj%10>broj/10%10 == rastecki(broj/10) ? broj%10>broj/10%10 : 0;
}

int main(){
    int n;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        int broj;
        scanf("%d",&broj);
        rastecki(broj) ? printf("DA\n") : printf("NE\n");
    }
    return 0;
}

// 6ta
#include <stdio.h>

int nzd(int a, int b) 
{ 
    if (a == 0) return b; 
    return nzd(b % a, a); 
} 

int main(){
    int a,b;
    scanf("%d %d",&a,&b);
    printf("%d",nzd(a,b));
    return 0;
}

// 7ma
int sumOfDigits(int broj){
    if(!broj) return broj;
    return broj%10 + sumOfDigits(broj/10);
}

int main()
{
    int n, i;
    scanf("%d", &n);
    int sum = 0;
    for(i = 0; i < n; ++i){

        int current;
        scanf("%d", &current);
        sum += sumOfDigits(current);
        printf("%d\n", sumOfDigits(current));
    }
    printf("%d", sum);
    return 0;
}

// 8ma
#include <stdio.h>

void linija(int broj,int brojac){
    if(broj==brojac) return;
    printf("%d ",++brojac);
    linija(broj,brojac);
}

void triagolnik(int broj){
    if(!broj) return;
    linija(broj,0);    
    printf("\n");
    triagolnik(--broj);
}

int main(){
    int n;
    scanf("%d",&n);
    triagolnik(n);
    return 0;
}

// 9ta
#include <stdio.h>

void linija(int broj,int brojac){
    if(broj==brojac) return;
    printf("%d",++brojac);
    linija(broj,brojac);
}

void triagolnik(int broj, int brojac){
    if(broj==brojac) return;
    linija(++brojac,0);    
    printf("\n");
    triagolnik(broj,brojac);
}

int main(){
    int n;
    scanf("%d",&n);
    triagolnik(n,0);
    return 0;
}
