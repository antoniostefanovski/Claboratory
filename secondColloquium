// 1va zadacha

#include <stdio.h>
#include <ctype.h>
#include <string.h>

void writeToFile() {
  FILE *f = fopen("text.txt", "w");
  char c;
  while((c = getchar()) != '#') {
    fputc(c, f);
  }
  fclose(f);
}

int isVowel (char c) {
    c = tolower (c);
    switch(c) {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
            return 1;
        default:
            return 0;
    }
}

int main() {
    
  writeToFile();
   
  FILE * inputFile = fopen("text.txt", "r");
    
    char line[100];
    
    int vowelCounter = 0;
    
    while(fgets(line,100,inputFile) != NULL) {
        int i;
        for(i=0; i<strlen(line); i++) {
            if(isVowel(line[i])) {
                if(isVowel(line[i+1])) {
                    vowelCounter++;
                    printf("%c%c\n", tolower(line[i]),tolower(line[i+1]));
                }
            }
        }
    }
    printf("%d", vowelCounter);
    fclose(inputFile);


  return 0;
}


// 2ra zadacha
int main()
{
    FILE * inputFile = fopen("broevi.txt", "r");
    
    int n, tmp;
    
    while (1) {
        fscanf(inputFile, "%d", &n);
        if (n == 0) {
            break;
        }
        int max = 0;
        int maxB;
        int i;
        for(i=0; i<n; i++) {
            int num;
            fscanf(inputFile, "%d",  &num);
            tmp = num;
            while(tmp > 9) {
                tmp /= 10;
            }
            if (tmp > max) {
                max = tmp;
                maxB = num;
            }
        }
        printf("%d\n", maxB);
    }
    
    fclose(inputFile);
}

// 8ma zadacha
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#define MAX 100
// ne menuvaj ovde
void wtf() {
    FILE *f = fopen("dat.txt", "w");
    char c;
    while((c = getchar()) != EOF) {
        fputc(c, f);
    }
    fclose(f);
}

int main() {
    wtf();
    FILE * inputFile = fopen("dat.txt", "r");
    
    char line[100], newLine[100];
    
    int digitCounter = 0, firstLine, lastLine = 0;
    int max = 0, firstLineRes, lastLineRes;
    
    while(fgets(line, 100, inputFile) != NULL) {
        int i;
        for(i=0; i<strlen(line); i++) {
            if(isdigit(line[i])) {
                firstLine = i;
                break;
            }
        }
        for(i=strlen(line); i>firstLine; i--) {
            if(isdigit(line[i])) {
                lastLine = i+1;
                break;
            }
        }
        if(lastLine - firstLine > max || lastLine - firstLine == max) {
            max = lastLine - firstLine;
            strcpy(newLine, line);
            firstLineRes = firstLine;
            lastLineRes = lastLine;
        }
        firstLine = 0;
        lastLine = 0;
    }
    
    int i;
    for(i=firstLineRes; i<lastLineRes; i++) {
        printf("%c", newLine[i]);
    }
    fclose(inputFile);
}
