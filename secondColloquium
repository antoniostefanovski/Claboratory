// 1va zadacha

#include <stdio.h>
#include <ctype.h>
#include <string.h>

void writeToFile() {
  FILE *f = fopen("text.txt", "w");
  char c;
  while((c = getchar()) != '#') {
    fputc(c, f);
  }
  fclose(f);
}

int isVowel (char c) {
    c = tolower (c);
    switch(c) {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
            return 1;
        default:
            return 0;
    }
}

int main() {
    
  writeToFile();
   
  FILE * inputFile = fopen("text.txt", "r");
    
    char line[100];
    
    int vowelCounter = 0;
    
    while(fgets(line,100,inputFile) != NULL) {
        int i;
        for(i=0; i<strlen(line); i++) {
            if(isVowel(line[i])) {
                if(isVowel(line[i+1])) {
                    vowelCounter++;
                    printf("%c%c\n", tolower(line[i]),tolower(line[i+1]));
                }
            }
        }
    }
    printf("%d", vowelCounter);
    fclose(inputFile);


  return 0;
}
