// 1va zadacha

#include <stdio.h>
#include <ctype.h>
#include <string.h>

void writeToFile() {
  FILE *f = fopen("text.txt", "w");
  char c;
  while((c = getchar()) != '#') {
    fputc(c, f);
  }
  fclose(f);
}

int isVowel (char c) {
    c = tolower (c);
    switch(c) {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
            return 1;
        default:
            return 0;
    }
}

int main() {
    
  writeToFile();
   
  FILE * inputFile = fopen("text.txt", "r");
    
    char line[100];
    
    int vowelCounter = 0;
    
    while(fgets(line,100,inputFile) != NULL) {
        int i;
        for(i=0; i<strlen(line); i++) {
            if(isVowel(line[i])) {
                if(isVowel(line[i+1])) {
                    vowelCounter++;
                    printf("%c%c\n", tolower(line[i]),tolower(line[i+1]));
                }
            }
        }
    }
    printf("%d", vowelCounter);
    fclose(inputFile);


  return 0;
}


// 2ra zadacha
int main()
{
    FILE * inputFile = fopen("broevi.txt", "r");
    
    int n, tmp;
    
    while (1) {
        fscanf(inputFile, "%d", &n);
        if (n == 0) {
            break;
        }
        int max = 0;
        int maxB;
        int i;
        for(i=0; i<n; i++) {
            int num;
            fscanf(inputFile, "%d",  &num);
            tmp = num;
            while(tmp > 9) {
                tmp /= 10;
            }
            if (tmp > max) {
                max = tmp;
                maxB = num;
            }
        }
        printf("%d\n", maxB);
    }
    
    fclose(inputFile);
}

// 8ma zadacha
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#define MAX 100
// ne menuvaj ovde
void wtf() {
    FILE *f = fopen("dat.txt", "w");
    char c;
    while((c = getchar()) != EOF) {
        fputc(c, f);
    }
    fclose(f);
}

int main() {
    wtf();
    FILE * inputFile = fopen("dat.txt", "r");
    
    char line[100], newLine[100];
    
    int digitCounter = 0, firstLine, lastLine = 0;
    int max = 0, firstLineRes, lastLineRes;
    
    while(fgets(line, 100, inputFile) != NULL) {
        int i;
        for(i=0; i<strlen(line); i++) {
            if(isdigit(line[i])) {
                firstLine = i;
                break;
            }
        }
        for(i=strlen(line); i>firstLine; i--) {
            if(isdigit(line[i])) {
                lastLine = i+1;
                break;
            }
        }
        if(lastLine - firstLine > max || lastLine - firstLine == max) {
            max = lastLine - firstLine;
            strcpy(newLine, line);
            firstLineRes = firstLine;
            lastLineRes = lastLine;
        }
        firstLine = 0;
        lastLine = 0;
    }
    
    int i;
    for(i=firstLineRes; i<lastLineRes; i++) {
        printf("%c", newLine[i]);
    }
    fclose(inputFile);
}

// 3ta
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int sum_pos (int * arr, int ind, int n) {
    int sum = 0;
    while(ind < n) {
        sum += *(arr+ind);
        ind++;
    }
    return sum;
}


int main()
{
    int n;
    scanf("%d", &n);
    int array[n];
    int i;
    for(i=0; i<n; i++) {
        scanf("%d", &array[i]);
    }

    int ind;
    scanf("%d", &ind);

    int suma = sum_pos(array, ind, n);
    printf("%d", suma);
    return 0;
}

// 10ta
#include <stdio.h>
#include <ctype.h>
// ne menuvaj ovde
void wtf() {
    FILE *f = fopen("dat.txt", "w");
    char c;
    while((c = getchar()) != '#') {
        fputc(c, f);
    }
    fclose(f);
}

int main() {
    wtf();
    char ch = getchar(); 
    char z1,z2;
    scanf("%c%c",&z1,&z2);
    FILE *f;
    if(!(f=fopen("dat.txt","r"))) printf("GRESKA");
    while(!feof(f)){
        char red[100];
        fgets(red,100,f);
        if(feof(f)) break;
        int print = 0;
        for(int i=0;i<100;i++){
            if(red[i]==z2) break;
            if(print) putchar(red[i]);
            if(red[i]=='#') break;
            if(red[i]==z1) print=1;

        }
        printf("\n");

    }

    return 0;
}

// 4ta 
#include <stdio.h>


int main()
{
    int N, M, i, j;
    scanf("%d %d", &N, &M);
    
    int matrix[N][M];
    
    for(i=0; i<N; i++) {
        for(j=0; j<M; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    
    int n, m;
    scanf("%d %d", &n, &m);
    
    int I = 0, II = 0, III = 0, IV = 0;
    
    for(i=0; i<N; i++) {
        for(j=0; j<M; j++) {
            if (i < n && j >= m) I += matrix[i][j];
            if (i < n && j < m) II += matrix[i][j];
            if (i >= n && j < m) III += matrix[i][j];
            if (i >= n && j >= m) IV += matrix[i][j];
        }
    }
    
    printf("%d %d %d %d", I,II,III,IV);
    return 0;
}

// 11ta zadacha
#include <stdio.h>
#include <string.h>
#include <ctype.h>
void wtf() {
    FILE *f = fopen("input.txt", "w");
    char c;
    while((c = getchar()) != EOF) {
        fputc(c, f);
    }
    fclose(f);
}

void sort(char niza[],int a){
    for(int i=0;i<a;i++){
        for(int j=0;j<a-1;j++){
            if(niza[j]>niza[j+1]){
                int temp=niza[j];
                niza[j]=niza[j+1];
                niza[j+1]=temp;
            }
        }
    }
}

int main() {
    wtf();
    FILE * inputFile = fopen("input.txt", "r");
    
    char line[80], line2[80];
    
    
    while(fgets(line,80,inputFile) != NULL) {
        int digitCounter = 0;
        int len = strlen(line);
        int i;
        for(i=0; i<len; i++) {
            if(isdigit(line[i])) {
                line2[digitCounter] = line[i];
                digitCounter++;
            }
        }
        if(digitCounter == 0) {
            continue;
        } 
        sort(line2, digitCounter);
        printf("%d:", digitCounter);
        for(i=0; i<digitCounter; i++) {
        printf("%c", line2[i]);
        }
        printf("\n");
    }
    fclose(inputFile);
    return 0;
}


// 5ta
#include <stdio.h>
#include <string.h>
#include <ctype.h>

// ne menuvaj ovde
void wf() {
    FILE *f = fopen("livce.txt", "w");
    char c;
    while((c = getchar()) != '#') {
        fputc(c, f);
    }
    fclose(f);
}

int main() {
    wf();
	FILE * inputFile = fopen("livce.txt", "r");
    
    char shifra[10];
    int tip; // 1 0 ili 2
    int suma;
    fscanf(inputFile,"%d", &suma);
    float koeficient;
    char maxShifra[10];
    int maxTip;
    float maxKoeficient;
    int flag = 1;
    float p = (float)suma;
    
    while(fscanf(inputFile,"%s",shifra) != EOF) {
        fscanf(inputFile, "%d", &tip);
        fscanf(inputFile, "%f", &koeficient);
        p = p*koeficient;
        
        if(flag) {
            strcpy(maxShifra,shifra);
            maxTip=tip;
            maxKoeficient=koeficient;
            flag=0;
        }
        else {
            if(koeficient > maxKoeficient) {
                strcpy(maxShifra,shifra);
                maxTip=tip;
                maxKoeficient=koeficient;
            }
        }
    }
    printf("%s %d %.2f\n%.2f",maxShifra,maxTip,maxKoeficient,p);
    fclose(inputFile);

	return 0;    

}

// 6ta
#include <stdio.h>

int main(){
    int N,M;
    scanf("%d %d", &N,&M);
    int mat[N][M];
    for(int i=0;i<N;i++){
        for(int j=0;j<M;j++){
            scanf("%d", &mat[i][j]);
        }
    }
    int vkupno=0,brojach;
    for(int i=0;i<N;i++){
        brojach=0;
        for(int j=0;j<M;j++){
            if(mat[i][j]==1){
                brojach++;
                if(brojach>=3){
                    vkupno++;
                    break;
                }
            }
            else{
                if(brojach>=3){
                    vkupno++;
                    break;
                }
                else{
                    brojach=0;
                }    
            }
        }
    }
    for(int i=0;i<M;i++){
        brojach=0;
        for(int j=0;j<N;j++){
            if(mat[j][i]==1){
                brojach++;
                if(brojach>=3){
                    vkupno++;
                    break;
                }
            }
            else{
                if(brojach>=3){
                    vkupno++;
                    break;
                }
                else{
                    brojach=0;
                }    
            }
        }
    }
    printf("%d", vkupno);
}
