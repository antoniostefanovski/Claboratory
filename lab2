//code

#include <stdio.h>
#include <stdlib.h>

int main()
{
    char c, d, e;
    scanf("%c %c %c", &c, &d, &e);
    if(c >= 'a' && c <= 'z' || c >= 'A' && c <= 'Z')
    {
        printf("1");
    }
    else if (c >= '0' && c <= '9') {
        printf("1");
    }
    else {
        printf("0");
    }
     if(d >= 'a' && d <= 'z' || d >= 'A' && d <= 'Z')
    {
        printf("1");
    }
    else if (d >= '0' && d <= '9') {
        printf("1");
    }
    else {
        printf("0");
    }
     if(e >= 'a' && e <= 'z' || e >= 'A' && e <= 'Z')
    {
        printf("1");
    }
    else if (e >= '0' && e <= '9') {
        printf("1");
    }
    else {
        printf("0");
    }
    printf("\n");
    //
       if(c >= 'a' && c <= 'z' || c >= 'A' && c <= 'Z')
    {
        printf("0");
    }
    else {
        printf("1");
    }

        if(d >= 'a' && d <= 'z' || d >= 'A' && d <= 'Z')
    {
        printf("0");
    }
    else {
        printf("1");
    }

        if(e >= 'a' && e <= 'z' || e >= 'A' && e <= 'Z')
    {
        printf("0");
    }
    else {
        printf("1");
    }


}
