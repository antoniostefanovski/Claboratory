// 1va zadacha
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

void printArray(int array[], int n);

int main()
{
    int n;
    scanf("%d", &n);
    int array[n];

    for (int i=0; i<n; i++) {
        scanf("%d", &array[i]);
    }

    printArray(array, n);

    for (int i=0; i<(n/2); i++) {
        int tmp = array[i];
        array[i] = array[n-1-i];
        array[n-1-i]=tmp;
    }

    printArray(array, n);

    for(int i=1;i<n;i++) {
        for(int j=i;j<n-1;j++) {
            array[j]=array[j+1];
        }
        --n;
    }

    printArray(array,n);

    for (int i=0; i<(n/2); i++) {
        int tmp = array[i];
        array[i] = array[n-1-i];
        array[n-1-i]=tmp;
    }

    printArray(array,n);

    return 0;
}

void printArray(int array[], int n) {
    for(int i=0;i<n;i++) {
        printf("%d ", array[i]);
    }
    printf("\n");
}

// 2ra zadacha
#include <stdio.h>

void elka(int n){
    int i=0;
    while(++i){
        for(int j=0;j<n;j++){
            if(j>n/2-i && j<n/2+i) printf("*");
            else printf(" ");
        }
        if(i==n/2+1) return;
        printf("\n");
    }
}

int main(){
    int k;
    scanf("%d",&k);
    elka(k);
    return 0;
}

// 3ta zadacha
#include <stdio.h>

void readMatrix(int matrix[100][100], int n, int m) {
    int i, j;
    for(i=0; i<n; i++) {
        for(j=0; j<m; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
}

int main()
{
    int matrix[100][100], n, m;

    scanf("%d %d", &n, &m);

    readMatrix(matrix, n, m);

    int min = 10000, poz = 0;

    int i,j;
    for(i=0; i<m; i++) {
        int sum = 0;
        for(j=0; j<n; j++) {
            sum += matrix[j][i];
        }
        if(sum < min) {
            min = sum;
            poz = i;
        }
    }
    printf("%d", poz);
    return 0;
}

// 4ta zadacha
#include <stdio.h>

int obratenBroj (int broj) {
    int obraten = 0;
    while(broj) {
        obraten = obraten * 10 + broj%10;
        broj/=10;
    }
    return obraten;
}

int sumOfDigit (int broj) {
    int sum = 0;
    while(broj) {
        sum+=broj%10;
        broj/=10;
    }
    return sum;
}

void pecatiVoInterval (int a, int b) {
    int i;
    for(i=a; i<= b; i++) {
        int zbir = i + obratenBroj (i);
        if((zbir) % sumOfDigit(zbir) == 0)
            printf("%d\n", i);
    }
}

int main()
{
    int a, b;
    scanf("%d %d", &a, &b);
    pecatiVoInterval(a, b);
    return 0;
}
